    "forever": "^0.10.11",
    "grunt": "~0.4.4",
    "grunt-contrib-sass": "~0.7.3",
    "mocha": "~1.18.0",
    "chai": "~1.9.0",
    "chai-as-promised": "~4.1.1",
    "grunt-contrib-clean": "^0.5.0",
